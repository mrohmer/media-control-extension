{
  "manifest_version": 2,
  "name": "MCR Media Control",
  "version": "1.0.0",
  "browser_action": {
    "default_popup": "pop-up.html"
  },
  "content_scripts": [
    {
      "matches": [
        "file://*"
      ],
      "js": [
        "build/injection.js"
      ],
      "css": [
        "build/injection.css"
      ]
    }
  ]
}
